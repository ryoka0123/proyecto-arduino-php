# ======================================
# Laravel 12 - Gitignore actualizado
# ======================================

# Dependencias de PHP y Node
/vendor/
node_modules/

# Archivos de entorno
.env
.env.*
!.env.example

# Logs y archivos temporales
npm-debug.log
yarn-error.log
pnpm-debug.log
composer.lock
.phpunit.result.cache
phpunit.result.cache

# Directorios de caché y almacenamiento
/storage/*.key
/storage/app/public
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
/storage/logs/*
/bootstrap/cache/*.php

# Laravel 4 specific
bootstrap/compiled.php
app/storage/

# Laravel 5+ specific
public/storage
public/hot

# Si cambiaste el path público
public_html/storage
public_html/hot

# Vagrant / Homestead
Homestead.yaml
Homestead.json
/.vagrant

# Archivos del sistema operativo
.DS_Store
Thumbs.db
ehthumbs.db
desktop.ini

# Configuración de IDEs
.idea/
.vscode/
*.sublime-workspace
*.sublime-project

# Archivos generados o temporales
*.bak
*.tmp
*.log
*.cache

# Archivos de cobertura o pruebas
coverage/
.phpunit.cache/

# Copias o bases de datos locales
*.sql
*.sqlite
/database/database.sqlite

# ======================================
# Carpetas personalizadas (ajusta según tu proyecto)
# ======================================
/public/uploads/
/public/temp/
